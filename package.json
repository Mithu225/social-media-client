    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "prettier": "3.3.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "babel-jest": "^29.7.0",
    "bootstrap-dark-5": "^1.1.3"
  },
  "jest": {
